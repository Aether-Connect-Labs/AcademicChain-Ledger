{
  "name": "remote-wsl",
  "displayName": "Remote - WSL",
  "description": "Full-featured support for developing in Windows Subsystem for Linux (WSL)",
  "version": "1.0.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-remote-wsl"
  },
  "publisher": "ms-vscode-remote",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./dist/extension.js",
  "icon": "resources/remote-wsl.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onResolveRemoteAuthority:wsl"
  ],
  "contributes": {
    // ... (el contenido original de contributes est√° bien)
